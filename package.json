  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "overrides": {
      "pinia": "$pinia",
      "rollup": "$rollup",
      "vite": "$vite",
      "vue": "$vue"
    },
    "ignoredBuiltDependencies": [
      "core-js",
      "esbuild"
    ]
  }
}
